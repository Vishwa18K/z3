{
  "name": "z3",
  "displayName": "Z3 Language",
  "description": "A syntax highlighter for the Z3 language by zcubes",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "z3",
      "aliases": ["z3", "z3"],
      "extensions": [".z3"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "z3",
      "scopeName": "source.zcubes.z3",
      "path": "./syntaxes/z3.tmLanguage.json"
    }]
  }
}
